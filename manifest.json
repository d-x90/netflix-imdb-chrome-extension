{
    "manifest_version": 2,
    "name": "NETFLIX with IMDb rating",
    "version": "0.1.0",
    "description": "Browse NETFLIX with IMDb ratings",
    "content_scripts": [
        {
            "css": [
                "styles.css"
            ],
            "js": [
                "content.js"
            ],
            "matches": [
                "https://www.netflix.com/browse"
            ]
        }
    ],
    "permissions": [
        "storage"
    ]
}